{
    "name": "BotTextDataset",
    "properties": {
        "type": "AzureDataLakeStore",
        "linkedServiceName": "DataLakeStoreLinkedService",
        "typeProperties": {
      "folderPath": "botdata/raw/{Year}/{Month}/{Day}",

      "partitionedBy": [
        {
          "name": "Year",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyy"
          }
        },
        {
          "name": "Month",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "MM"
          }
        },
        {
          "name": "Day",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "dd"
          }
        }
      ],
            "format": {
                "type": "JsonFormat"
            }
        },
        "availability": {
            "frequency": "Day",
            "interval": 1,
            "style": "StartOfInterval"
        },
        "external": true,
        "policy": {}
    }
}
